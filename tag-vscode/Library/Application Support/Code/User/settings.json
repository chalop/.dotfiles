  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Character escape",
        "scope": "constant.character.escape",
        "settings": {
          "foreground": "#f05050"
        }
      },
      {
        "name": "Graphql String",
        "scope": "string.quoted.double.graphql",
        "settings": {
          "fontStyle": "bold",
          "foreground": "#FFF"
        }
      },
      {
        "scope": "entity.name.tag.js",
        "settings": {
          "foreground": "#da27b3"
        }
      },
      {
        "scope": "entity.name.tag.ts",
        "settings": {
          "foreground": "#da27b3"
        }
      },
      {
        "scope": "entity.name.tag.tsx",
        "settings": {
          "foreground": "#da27b3"
        }
      },
      {
        "scope": "entity.name.tag.jsx",
        "settings": {
          "foreground": "#da27b3"
        }
      },
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "keyword.control.conditional.js",
          "keyword.control.switch.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          // "meta.arrow.js",
          // "meta.block.js",
          // "meta.embedded.expression.js",
          // "meta.jsx.children.js",
          // "meta.tag.attributes.js",
          // "meta.tag.js",
          // "meta.tag.without-attributes.js",
          // "meta.var.expr.js",
          // "source.js",
          // "variable.other.readwrite.js"
        ],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#FF0000"
        }
      }
    ]
  },
